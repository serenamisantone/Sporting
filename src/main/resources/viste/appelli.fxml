<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="it.univaq.disim.oop.myunivaq.controller.AppelliController">
   <children>
      <Label fx:id="appelliLabel" prefHeight="41.0" prefWidth="580.0" text="Elenco Appelli: ">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button mnemonicParsing="false" onAction="#aggiungiAppelloAction" prefHeight="30.0" prefWidth="115.0" text="Aggiungi Appello" />
      <TableView fx:id="tabellaAppelli" prefHeight="299.0" prefWidth="580.0">
        <columns>
          <TableColumn fx:id="descrizioneTableColumn" prefWidth="213.0" text="Descrizione" />
          <TableColumn fx:id="dataTableColumn" prefWidth="156.0" text="Data Appello" />
            <TableColumn fx:id="tipologiaEsameTableColumn" prefWidth="136.0" text="Tipologia Esame" />
            <TableColumn fx:id="azioniTableColumn" prefWidth="75.0" text="Azioni" />
        </columns>
      </TableView>
   </children>
   <opaqueInsets>
      <Insets />
   </opaqueInsets>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
